{
  "name": "uun",
  "version": "1.0.0",
  "description": "A website created using 11ty, Slinkity, Sass and Tailwind.",
  "main": ".eleventy.js",
  "scripts": {
    "clean": "rimraf _site",
    "serve:css": "tailwindcss -i src/assets/css/index.scss -o _site/assets/css/index.css --watch --postcss",
    "serve:eleventy": "ELEVENTY_ENV=development eleventy --serve",
    "serve": "npm-run-all clean --parallel serve:*",
    "build:css": "tailwindcss -i src/assets/css/index.scss -o _site/assets/css/index.css --postcss",
    "build:eleventy": "ELEVENTY_ENV=production eleventy",
    "build": "NODE_ENV=production run-s clean build:*"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@builder.io/partytown": "^0.7.3",
    "autoprefixer": "^10.4.12",
    "daisyui": "^2.31.0",
    "dotenv": "^16.0.3",
    "elasticlunr": "^0.9.5",
    "figlet": "^1.5.2",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-footnote": "^3.0.3",
    "postcss-cli": "^10.0.0",
    "theme-change": "^2.2.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@slinkity/renderer-svelte": "^1.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "postcss-scss": "^4.0.6",
    "sass": "^1.55.0",
    "slinkity": "^0.8.3",
    "svelte": "^3.50.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
